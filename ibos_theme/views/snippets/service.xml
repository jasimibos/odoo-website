<odoo>
    <template id="service_feed_sec" inherit_id="website.snippets" name="Service Feed ">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="ibos_theme.service_feed"/>
